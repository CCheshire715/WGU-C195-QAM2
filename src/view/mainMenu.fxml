<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="350.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <children>
      <VBox alignment="CENTER" layoutX="25.0" layoutY="50.0" prefHeight="250.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="appointmentsButton" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionAppointmentsButton" prefWidth="100.0" text="Appointments" />
            <Button fx:id="customersButton" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionCustomersButton" prefWidth="100.0" text="Customers" />
            <Button fx:id="reportsButton" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionReportsButton" prefWidth="100.0" text="Reports" />
         </children>
      </VBox>
      <Button fx:id="mainMenuLogOut" layoutX="176.0" layoutY="311.0" maxWidth="-Infinity" mnemonicParsing="false" onAction="#onActionLogOut" prefWidth="60.0" text="Log out" />
   </children>
</AnchorPane>
